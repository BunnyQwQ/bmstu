#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
void fibstr(int n)
{
	char* ptr1 = (char*)malloc(2);
	int l1 = 1;
	strcpy(ptr1, "a");
	char* ptr2 = (char*)malloc(2);
	int l2 = 1;
	strcpy(ptr2, "b");
	char* ptr3 = (char*)malloc(3);
	int l3 = 2;
	strcpy(ptr3, "ab");
	int i = 4;
	while (i <= n)
	{
		switch (i % 3)
		{
		case 1:
			l1 = l2 + l3;
			free(ptr1);
			ptr1 = (char*)malloc(l1 + 1);
			strcpy(ptr1, ptr2);
			strcat(ptr1, ptr3);
			break;
		case 2:
			l2 = l3 + l1;
			free(ptr2);
			ptr2 = (char*)malloc(l2 + 1);
			strcpy(ptr2, ptr3);
			strcat(ptr2, ptr1);
			break;
		case 0:
			l3 = l1 + l2;
			free(ptr3);
			ptr3 = (char*)malloc(l3 + 1);
			strcpy(ptr3, ptr1);
			strcat(ptr3, ptr2);
			break;
		default:
			break;
		}
		i++;
	}
	if (n % 3 == 2) {
		printf("%s", ptr2); return;
	}
	if (n % 3 == 1) {
		printf("%s", ptr1); return;
	}
	if (n % 3 == 0) { printf("%s", ptr3); return; }
}





int main()
{
	int n;
	scanf("%d", &n);
	if (n > 2)
		fibstr(n);
	else if (n == 1) printf("a");
	else if (n == 2) printf("b");
	else printf("n <= 0");
}
